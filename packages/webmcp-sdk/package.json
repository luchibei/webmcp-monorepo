{
  "name": "@luchibei/webmcp-sdk",
  "version": "0.1.0",
  "description": "Core SDK primitives for WebMCP tools",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --watch --config tsup.config.ts",
    "test": "vitest run --config ../../vitest.config.ts",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "dependencies": {
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
